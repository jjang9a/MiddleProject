<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.start.mapper.ProductMapper">
<!-- 가애 -->


<!-- 순덕 -->
<select id="packageList" resultType="co.start.vo.ProductVO">
	select * from product
	where pd_type = 'PK' and pd_location='서울 경기'
	order by pd_start
</select>

<select id="packageInfo" resultType="co.start.vo.ProductVO">
	select * from product
	where pd_id=10001 and  pd_type='PK';
</select>


<!-- 용억 -->
  <select id="productList" resultType="co.start.vo.ProductVO">
    select * from product
  </select>
  <select id="productInfo" resultType="co.start.vo.ProductVO">
  	select * from product where pd_id =#{pdId}
  </select>


<!-- 대준 -->

</mapper>